# Focus display
alt - x : yabai -m display --focus 1
alt - c : yabai -m display --focus 2
alt - v : yabai -m display --focus 3

# Focus space
alt - 1 : yabai -m space --focus dev0
alt - 2 : yabai -m space --focus dev1
alt - 3 : yabai -m space --focus dev2
alt - 4 : yabai -m space --focus web0
alt - 5 : yabai -m space --focus web1
alt - 6 : yabai -m space --focus web2
alt - 7 : yabai -m space --focus tools0
alt - 8 : yabai -m space --focus tools0
alt - 9 : yabai -m space --focus misc0
alt - 0 : yabai -m space --focus misc1
# alt - 1 : yabai -m space --focus 1
# alt - 2 : yabai -m space --focus 2
# alt - 3 : yabai -m space --focus 3
# alt - 4 : yabai -m space --focus 4
# alt - 5 : yabai -m space --focus 5
# alt - 6 : yabai -m space --focus 6
# alt - 7 : yabai -m space --focus 7
# alt - 8 : yabai -m space --focus 8
# alt - 9 : yabai -m space --focus 9
# alt - 0 : yabai -m space --focus 0

# Move space
shift + alt - x : yabai -m space --display 1
shift + alt - c : yabai -m space --display 2
shift + alt - v : yabai -m space --display 3

# Focus window
alt - h : yabai -m window --focus west
alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north
alt - l : yabai -m window --focus east
alt - g : yabai -m window --focus first

# Resize window
# ctrl + alt - h : yabai -m window --resize left:-100:0; yabai -m window --resize right:-100:0
# ctrl + alt - j : yabai -m window --resize top:0:100; yabai -m window --resize bottom:0:100
# ctrl + alt - k : yabai -m window --resize top:0:-100; yabai -m window --resize bottom:0:-100
# ctrl + alt - l : yabai -m window --resize left:100:0; yabai -m window --resize right:100:0

# Swap window
shift + alt - h : yabai -m window --swap west
shift + alt - j : yabai -m window --swap south
shift + alt - k : yabai -m window --swap north
shift + alt - l : yabai -m window --swap east

# Warp window
shift + ctrl + alt - h : yabai -m window --warp west
shift + ctrl + alt - j : yabai -m window --warp south
shift + ctrl + alt - k : yabai -m window --warp north
shift + ctrl + alt - l : yabai -m window --warp east

# Move window to space
# shift + alt - 1 : yabai -m window --space dev0; yabai -m space --focus dev0
# shift + alt - 2 : yabai -m window --space dev1; yabai -m space --focus dev1
# shift + alt - 3 : yabai -m window --space dev2; yabai -m space --focus dev2
# shift + alt - 4 : yabai -m window --space web0; yabai -m space --focus web0
# shift + alt - 5 : yabai -m window --space web1; yabai -m space --focus web1
# shift + alt - 6 : yabai -m window --space web2; yabai -m space --focus web2
# shift + alt - 7 : yabai -m window --space tools0; yabai -m space --focus tools0
# shift + alt - 8 : yabai -m window --space tools1; yabai -m space --focus tools1
# shift + alt - 9 : yabai -m window --space misc0; yabai -m space --focus misc0
# shift + alt - 0 : yabai -m window --space misc1; yabai -m space --focus misc1
shift + alt - 1 : yabai -m window --space dev0;
shift + alt - 2 : yabai -m window --space dev1;
shift + alt - 3 : yabai -m window --space dev2;
shift + alt - 4 : yabai -m window --space web0;
shift + alt - 5 : yabai -m window --space web1;
shift + alt - 6 : yabai -m window --space web2;
shift + alt - 7 : yabai -m window --space tools0;
shift + alt - 8 : yabai -m window --space tools1;
shift + alt - 9 : yabai -m window --space misc0;
shift + alt - 0 : yabai -m window --space misc1;

# Flip
shift + alt - n : yabai -m space --mirror x-axis
shift + alt - m : yabai -m space --mirror y-axis

# Balace space
ctrl + alt - space : yabai -m space --balance

# Toggle zoom
alt - space : yabai -m window --toggle zoom-fullscreen

# Toggle split
shift + alt - space : yabai -m window --toggle split

# Toggle float
alt - f : yabai -m window --toggle float
